{
    "compilerOptions": {
      "target": "es6",
      "module": "es2020",
      "strict": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "allowJs": true,
      "jsx": "preserve"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules/*"]
  }
  
  